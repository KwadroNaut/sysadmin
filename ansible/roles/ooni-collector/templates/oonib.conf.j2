# do not touch. generated by ansible
main:
  archive_dir: {{ ooni_collector_archive_path }}
  bouncer_file: null
  chroot: null
  debug: false
  deck_dir: {{ ooni_collector_deck_path }}
  euid: null
  gid: null
  input_dir: {{ ooni_collector_input_path }}
  logfile: {{ ooni_collector_log_path }}
  no_save: true
  nodaemon: true
  originalname: null
  pidfile: oonib.pid
  policy_file: null
  profile: null
  report_dir: {{ ooni_collector_report_path }}
  rundir: .
  socks_port: 9055
  stale_time: 3600
  tor2webmode: false
  tor_binary: null
  tor_datadir: {{ ooni_collector_tor_data_path }}
  tor_hidden_service: true
  uid: null
  umask: null
  collector_endpoints:
  - type: tls
    port: {{ tls_collector_port | default ('4441') }}
    fullchain: "{{ tls_collector_fullchain  }}"
    privkey: "{{ tls_collector_privkey }}"}
  - type: tcp
    port: {{ tcp_collector_port | default ('8081') }}}
  - type: onion
    hsdir: "{{ collector_hsdir }}"
  report_file_template: '{iso8601_timestamp}-{test_name}-{report_id}-{probe_asn}-{probe_cc}-probe-0.2.0.{ext}'

# These are all disabled
helpers:
  daphn3:
    address: null
    pcap_file: null
    port: null
    yaml_file: null
  dns:
    address: null
    resolver_address: null
    tcp_port: null
    udp_port: null
  dns_discovery:
    address: null
    resolver_address: null
    tcp_port: null
    udp_port: null
  http-return-json-headers:
    address: null
    port: null
    server_version: Apache
  ssl:
    address: null
    certificate: certificate.crt
    private_key: private.key
    port: null
  tcp-echo:
    address: null
    port: null
  web_connectivity:
    endpoints: []
